<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="153.0" prefWidth="466.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.GerenciarPrecosViewController">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Gerenciar preços">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="61.0" text="Preço da hora:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="txtHora" layoutX="138.0" layoutY="62.0" />
            <Button fx:id="btSalvar" layoutX="24.0" layoutY="104.0" mnemonicParsing="false" onAction="#onBtSalvarAction" text="Salvar" />
            <Button fx:id="btCancelar" layoutX="83.0" layoutY="104.0" mnemonicParsing="false" onAction="#onBtCancelarAction" text="Cancelar" />
         </children>
      </AnchorPane>
   </children>
</VBox>
